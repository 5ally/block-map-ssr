{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tda/block-map-ssr",
	"version": "0.1.0",
	"title": "Mapbox GL JS demo (SSR)",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false,
		"multiple": false
	},
	"textdomain": "tda-map",
	"editorScript": "file:./index.js",
	"style": [ "file:./style-index.css", "mapbox-gl" ],
	"viewScript": [ "file:./view.js", "mapbox-gl" ],
	"attributes": {
		"accessToken": {
			"type": "string",
			"default": "",
			"source": "attribute",
			"selector": "div#map",
			"attribute": "data-token"
		}
	}
}